<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes" thread-count="5">
	<listeners>
		<listener class-name="greenKart.Listeners"></listener>
	</listeners>
	<test name="Validate Page Actions">
		<classes>
			<class name="greenKart.ValidatePageActions"></class>
		</classes>
	</test>
	<test name="Validate Cart Actions">
		<classes>
			<class name="greenKart.ValidateCartActions"></class>
		</classes>
	</test>
	<test name="Validate GreenKart Page">
		<classes>
			<class name="greenKart.ValidateGreenkartPage" />
			<class name="greenKart.ExternalLinks"></class>
		</classes>
	</test>
	<!-- Test -->
</suite> <!-- Suite -->
